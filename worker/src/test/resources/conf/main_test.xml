<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>
    <context:component-scan base-package="com.v5.test.worker" />

    <context:property-placeholder location="conf/main.properties" />

    <import resource="base_server.xml" />

    <import resource="base_service.xml" />

    <import resource="client_test.xml" />

    <import resource="reactor.xml" />

   <import resource="metrics.xml" />

</beans>
